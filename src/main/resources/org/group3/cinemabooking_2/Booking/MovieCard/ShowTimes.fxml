<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="300.0" prefWidth="190.0" stylesheets="@../../CSS/Booking/MovieCard/ShowTimes.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.group3.cinemabooking_2.Booking.MovieCard.ShowTimesController">
    <VBox onMouseClicked="#onBookingDetail" prefHeight="300.0" prefWidth="190.0" styleClass="backgroundAP">
        <ImageView fx:id="posterMovie" fitHeight="232.0" fitWidth="198.0" pickOnBounds="true" preserveRatio="true">

        </ImageView>
        <VBox alignment="CENTER_LEFT" prefHeight="88.0" prefWidth="194.0">
         <Label fx:id="movieName" styleClass="movieName" text="Label" />
         <Label fx:id="categoryMovie" styleClass="ratingText" text="Label" />
         <Label fx:id="author" styleClass="ratingText" text="Label" />
        </VBox>
    </VBox>
</AnchorPane>
